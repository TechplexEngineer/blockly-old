    "Maze.maze": "\ubbf8\ub85c",
    "Maze.instructions1": "\ud504\ub85c\uadf8\ub7a8\uc740 \ube14\ub85d\uc758 \uc21c\uc11c\uc785\ub2c8\ub2e4. \ubaa9\ud45c\uc5d0 \ub3c4\ub2ec\ud560 \uc218 \uc788\ub3c4\ub85d \ud568\uaed8 '\uc55e\uc73c\ub85c \uac00\uae30' \ube14\ub85d\uc744 \uc138\ub85c\ub85c \uc313\uc73c\uc138\uc694.",
    "Maze.instructions2": "\uc774 \uae38\uc744 \ub530\ub77c \uac00\uae30 \uc704\ud55c \ub2e8\uacc4\uc758 \uc21c\uc11c\ub294 \ubb34\uc5c7\uc785\ub2c8\uae4c?",
    "Maze.instructions3": "\ucef4\ud4e8\ud130\ub294 \uc81c\ud55c\ub41c \uba54\ubaa8\ub9ac\uac00 \uc788\uc2b5\ub2c8\ub2e4. \ube14\ub85d \ub450 \uac1c\ub9cc \uc0ac\uc6a9\ud574 \uc774 \uae38\uc758 \ub05d\uc5d0 \ub3c4\ub2ec\ud558\uc138\uc694. \ud55c \ubc88 \uc774\uc0c1 \ube14\ub85d\uc744 \uc2e4\ud589\ud558\ub824\uba74 '\ubc18\ubcf5'\uc744 \uc0ac\uc6a9\ud558\uc138\uc694.",
    "Maze.instructions4": "\ube14\ub85d \ub2e4\uc12f \uac1c\ub9cc \uc0ac\uc6a9\ud574\uc11c \ubaa9\ud45c\uc5d0 \ub3c4\ub2ec\ud558\uc138\uc694.",
    "Maze.instructions5": "\ud398\uadf8\ub9e8\uc740 \uc9c1\uc9c4\ud560 \uc218 \uc5c6\uc744 \ub54c \uc67c\ucabd\uc73c\ub85c \ub3cc\uc544\uc57c \ud569\ub2c8\ub2e4.",
    "Maze.instructions6": "'\ub9cc\uc57d' \ube14\ub85d\uc740 \uc870\uac74\uc774 \ucc38\uc774\uba74 \ubb34\uc5b8\uac00\ub97c \ud569\ub2c8\ub2e4. \uc67c\ucabd\uc5d0 \uae38\uc774 \uc788\uc73c\uba74 \uc67c\ucabd\uc73c\ub85c \ub3cc\ub3c4\ub85d \ud558\uc138\uc694.",
    "Maze.instructions7": "\uc774 \ubbf8\ub85c\ub294 \uc774\uc804 \ubbf8\ub85c\ubcf4\ub2e4 \ub354 \ubcf5\uc7a1\ud574 \ubcf4\uc774\uc9c0\ub9cc, \uadf8\ub807\uc9c0 \uc54a\uc2b5\ub2c8\ub2e4.",
    "Maze.instructions8": "\ud558\ub098 \uc774\uc0c1\uc758 '\ub9cc\uc57d' \ube14\ub85d\uc744 \uc0ac\uc6a9\ud560 \uc218 \uc788\uc2b5\ub2c8\ub2e4.",
    "Maze.instructions9": "\ub9cc\uc57d-\uadf8\ub807\uc9c0 \uc54a\uc73c\uba74 \ube14\ub85d\uc740 \ud55c \uac00\uc9c0 \ub610\ub294 \ub2e4\ub978 \ud589\ub3d9\uc744 \ud569\ub2c8\ub2e4.",
    "Maze.instructions10": "\ubcf5\uc7a1\ud55c \ubbf8\ub85c\ub97c \ud480 \uc218 \uc788\uc2b5\ub2c8\uae4c? \ub2e4\uc74c \uc67c\ucabd \ubcbd\uc744 \ud558\uc138\uc694. \uace0\uae09 \ud504\ub85c\uadf8\ub798\uba38\ub9cc!",
